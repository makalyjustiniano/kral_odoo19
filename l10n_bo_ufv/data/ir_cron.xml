<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="ir_cron_load_ufv_cotizations" model="ir.cron">
            <field name="name">Sincronizar cotizaciones UFV</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="model_id" ref="base.model_res_currency"/>
            <!-- Highest priority at time of module install. -->
            
            <field name="active" eval="True" />
            <field name="priority" eval="0"/>
            <field name="state">code</field>
            <field name="code">model.action_daily_ufv_cotization_request()</field>
        </record>
    </data>
</odoo>
