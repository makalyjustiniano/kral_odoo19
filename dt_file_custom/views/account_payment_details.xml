<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record model="ir.ui.view" id="view_order_form_inherit_payment">
            <field name="name">move.account.from.inherit.payment</field>
            <field name="model">account.payment</field>
            <field name="inherit_id" ref="account.view_account_payment_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_bank_id']" position="before">
                   
                     <field name="type_partner" string="Tipo de Comprobante" invisible="1" />
                    <field name="payment_type" string="Comprobante" invisible="1" />

                </xpath>
            </field>
        </record>
    </data>
</odoo>